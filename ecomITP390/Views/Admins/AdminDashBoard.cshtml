
@{
    ViewData["Title"] = "لوحة التحكم";

}

<h2>AdminDashBoard</h2>

    
 <div class="row ">
        <div class=" col-xl-4 col-lg-4 col-md-6 col-sm-6">
            <div class="col-sm-6 col-md-3">
                <a asp-controller="users" asp-action="index">
                    <span class="glyphicon glyphicon-user gi-5x" aria-hidden="true"></span>
                    <div class="caption">
                        <h4>المستخدمون</h4>
                    </div>
                </a>
            </div>
        </div>
        <div class="col-xl-4 col-lg-4 col-md-6 col-sm-6">
            <div class="col-xs-6 col-md-3">
                <a asp-controller="employees" asp-action="index">
                    <span class="glyphicon glyphicon-earphone gi-5x" aria-hidden="true"></span>
                    <div class="caption">
                        <h4>خدمة العملاء</h4>
                    </div>
                </a>
            </div>
        </div>
        <div class="col-xl-4 col-lg-4 col-md-6 col-sm-6">
            <div class="col-xs-6 col-md-3">
                <a asp-controller="Services" asp-action="index">
                    <span class="glyphicon glyphicon-shopping-cart gi-5x" aria-hidden="true"></span>
                    <div class="caption">
                        <h4>االخدمات المتاحة</h4>
                    </div>
                </a>
            </div>
        </div>
        <div class="col-xl-4 col-lg-4 col-md-6 col-sm-6">
            <div class="col-xs-6 col-md-3">
                <a asp-controller="orders" asp-action="index">
                    <span class="glyphicon glyphicon-list gi-5x" aria-hidden="true"></span>
                    <div class="caption">
                        <h4>المناقلات</h4>
                    </div>
                </a>
            </div>
        </div>
        <div class="col-xl-4 col-lg-4 col-md-6 col-sm-6">
            <div class="col-xs-6 col-md-3">
                <a asp-controller="Reports" asp-action="index">
                    <span class="glyphicon glyphicon-alert gi-5x" aria-hidden="true"></span>
                    <div class="caption">
                        <h4>البلاغات</h4>
                    </div>
                </a>
            </div>
        </div>
        <div class="col-xl-4 col-lg-4 col-md-6 col-sm-6">
            <div class="col-xs-6 col-md-3">
                <a asp-controller="disputes" asp-action="index">
                    <span class="glyphicon glyphicon-hand-up gi-5x" aria-hidden="true"></span>
                    <div class="caption">
                        <h4>النزاعات</h4>
                    </div>
                </a>
            </div>
        </div>
        <div class="col-xl-4 col-lg-4 col-md-6 col-sm-6">
            <div class="col-xs-6 col-md-3">
                <a asp-controller="orders" asp-action="index">
                    <span class="glyphicon glyphicon-star gi-5x" aria-hidden="true"></span>
                    <div class="caption">
                        <h4>المناقلات</h4>
                    </div>
                </a>
            </div>
        </div>

    </div>

